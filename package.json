{
  "name": "typescript-snowpack-template",
  "version": "1.0.0",
  "description": "A template for applications written in TypeScript, using Snowpack as the bundling system",
  "main": "app.js",
  "scripts": {
    "build": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline --watch",
		"check": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Syver Storm-Furru",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "snowpack": "^1.0.2",
    "typescript": "^3.7.4"
  },
  "dependencies": {}
}
